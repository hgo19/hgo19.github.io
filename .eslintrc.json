{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["trybe-frontend"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {
        "react-hooks/rules-of-hooks": "error", 
        "react-hooks/exhaustive-deps": "warn",
        "react/jsx-no-constructed-context-values": 0
    }
}
